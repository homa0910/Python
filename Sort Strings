for i in dataStr:
  lwrCaseStr.append (i.lower())

 
sortedStr = sorted(lwrCaseStr)


# Write output in a text file
fileName = 'C:\\repo\\sdks\Python34\\Thao Exercises\\Data\\data1.txt'
